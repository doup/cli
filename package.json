{
  "name": "doup-cli",
  "description": "doup cli utilities",
  "version": "0.1.0",
  "author": "doup @doup",
  "bin": {
    "dp": "./bin/run"
  },
  "bugs": "https://github.com/doup/doup-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.8.8",
    "@oclif/plugin-help": "^2.1.3",
    "@types/puppeteer": "^1.12.4",
    "csv": "^4.0.0",
    "glob": "^7.1.3",
    "puppeteer": "^1.17.0",
    "tslib": "^1.9.3",
    "yaml": "^1.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.0",
    "@oclif/test": "^1.2.1",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.7",
    "chai": "^4.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/doup/doup-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "doup/doup-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
